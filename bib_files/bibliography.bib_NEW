@Article{Wasserman2008,
  author         = {Wasserman, Adam and Maitra, Neepa T. and Heller, Eric J.},
  title          = {Investigating interaction-induced chaos using time-dependent density-functional theory},
  journal        = {PHYSICAL REVIEW A},
  year           = {2008},
  volume         = {77},
  number         = {4},
  pages          = {042503},
  month          = {APR},
  issn           = {1050-2947},
  abstract       = {Systems whose underlying classical dynamics are chaotic exhibit signatures of the chaos in their quantum mechanics. We investigate the possibility of using the linear response formalism of time-dependent density functional theory (TDDFT) to study the case when chaos is induced by electron-interaction alone. Nearest-neighbor level-spacing statistics are in principle exactly and directly accessible from TDDFT. We discuss how the TDDFT linear response procedure can reveal information about the mechanism of chaos induced by electron-interaction alone. A simple model of a two-electron quantum dot highlights the necessity to go beyond the adiabatic approximation in TDDFT.},
  article-number = {042503},
  doi            = {10.1103/PhysRevA.77.042503},
  file           = {:PAPERS/PhysRevA_77_042503.pdf:PDF},
  groups         = {Time-Resolved Phenomena, 2elSystems, Failures},
  keywords-plus  = {COMPLEX ATOMIC SPECTRA; ENERGY-LEVELS; STATISTICAL PROPERTIES; INTEGER DISCONTINUITY; EXCITATION-ENERGIES; LINEAR-RESPONSE; EXCHANGE; SYSTEMS; STATES; FLUCTUATIONS},
  owner          = {myrta},
