@Unpublished{Perakisv2000,
  author    = {I. E. Perakisv and T. V. Shahbazyan},
  title     = {Many-body Correlation Efects in the Ultrafast Nonlinear Optical Response of Confned Fermi Seas},
  note      = {arXiv:cond-mat/0001279v1},
  abstract  = {The dynamics of electrons and atoms interacting with intense and
ultrashort optical pulses presents an important problem in physics
that cuts across di?erent materials such as semiconductors and met-
als. The currently available laser pulses, as short as 5 fs, provide a
time resolution shorter than the dephasing and relaxation times in
many materials. This allows for a systematic study of many?body
e?ects using nonlinear optical spectroscopy. In this review article, we
discuss the role of Coulomb correlations in the ultrafast dynamics of
modulation?doped quantum wells and metal nanoparticles. We focus
in particular on the manifestations of non?Markovian memory e?ects
induced by strong electron?hole and electron?plasmon correlations.},
