@Other{Vignale2006,
  author    = {Vignale, G.},
  title     = {Current Density Functional Theory},
  year      = {2006},
  abstract  = {The nonlocality of the exchange-correlation (xc) potential, i.e., the fact that the xc potential at a certain position depends on the global distribution of the particle density in space, is the curse of density functional theory. It is mainly because of this fact that, even after years of intensive studies, the exact form of the xc potential as a functional of the density remains unknown. Nevertheless, it is true that many accurate and useful results can be obtained from the use of an approximation ? the local density approximation (LDA) ? which ignores the problem altogether. Apparently, the nonlocal dependence of the Kohn-Sham orbitals on the density is sufficient in many cases to give the right quantum chemistry. Furthermore, a number of successful strategies have been designed to go beyond the LDA when needed: in one such approach (the generalized gradient approximation ? GGA) one goes beyond the LDA by including the dependence of the xc potential on the gradient of the local density; in another one expresses the xc potential as a functional of the Kohn-Sham orbitals, and, finally, in the ?meta-GGA? approach one fights the problem by including additional local variables, such as the kinetic energy density.},
  file      = {:PAPERS/VignaleTDDFTBook.pdf:PDF},
  groups    = {Time-Resolved Phenomena},
  journal   = {Time-Dependent Density Functional Theory},
  owner     = {myrta},
  pages     = {75--91},
  review    = {Concept of ultranonlocality (divergence integral over space of the xc kernel)
HPT theorem and explanation failure of simple Gross approximation

analogy with hydrodynamics: reason why choosing current-vector potential instead of density-scalar potential
approximation homogeneous and inhomogeneous el gas},
  timestamp = {2009.11.02},
  url       = {http://dx.doi.org/10.1007/3-540-35426-3_5},
}

@Article{Vignale1997,
  author    = {Vignale, G. and Ullrich, C. A. and Conti, S.},
  title     = {Time-Dependent Density Functional Theory Beyond the Adiabatic Local Density Approximation},
  journal   = {Phys. Rev. Lett.},
  year      = {1997},
  volume    = {79},
  number    = {24},
  pages     = {4878--},
  month     = dec,
  abstract  = {In the current density functional theory of linear and nonlinear time-dependent phenomena, the treatment of exchange and correlation beyond the level of the adiabatic local density approximation is shown to lead to the appearance of viscoelastic stresses in the electron fluid. Complex and frequency-dependent viscosity/elasticity coefficients are microscopically derived and expressed in terms of properties of the homogeneous electron gas. As a first consequence of this formalism, we provide an explicit formula for the linewidths of collective excitations in electronic systems.},
  file      = {:PAPERS/PhysRevLett.79.4878.pdf:PDF},
  groups    = {Time-Resolved Phenomena, Exact_vs_Adiabatic, BeyondAdiabatic},
  owner     = {myrta},
  publisher = {American Physical Society},
  refid     = {10.1103/PhysRevLett.79.4878},
  timestamp = {2009.10.09},
  url       = {http://link.aps.org/abstract/PRL/v79/p4878},
}

