@Article{Shih2009,
  author      = {Shih, T. and Winkler, M. and Voss, T. and Mazur, E.},
  title       = {Dielectric function dynamics during femtosecond laser excitation of bulk?ZnO},
  journal     = {Applied Physics A: Materials Science \& Processing},
  year        = {2009},
  volume      = {96},
  pages       = {363-367},
  issn        = {0947-8396},
  note        = {10.1007/s00339-009-5196-0},
  abstract    = {Using a broad band dual-angle pump-probe reflectometry technique, we obtained the ultrafast dielectric function dynamics of bulk ZnO under femtosecond laser excitation. We determined that multiphoton absorption of the 800-nm femtosecond laser excitation creates a large population of excited carriers with excess energy. Screening of the Coulomb interaction by the excited free carriers causes damping of the exciton resonance and renormalization of the band gap causing broadband (2.3?3.5 eV) changes in the dielectric function of ZnO. From the dielectric function, many transient material properties, such as the index of refraction of ZnO under excitation, can be determined to optimize ZnO-based devices.},
  affiliation = {Harvard University Department of Physics and School of Engineering and Applied Sciences 9 Oxford Street Cambridge MA 02138 USA},
  file        = {:PAPERS/Mazur_639.pdf:PDF},
  groups      = {ZnO, Time-Resolved Phenomena},
  issue       = {2},
  keyword     = {Physics and Astronomy},
  publisher   = {Springer Berlin / Heidelberg},
  url         = {http://dx.doi.org/10.1007/s00339-009-5196-0},
}

@Article{Shimano2011,
  author    = {Shimano, Ryo and Suzuki, Takeshi},
  title     = {Exciton Mott transition in Si studied by terahertz spectroscopy},
  journal   = {physica status solidi (c)},
  year      = {2011},
  volume    = {8},
  number    = {4},
  pages     = {1153--1156},
  issn      = {1610-1642},
  abstract  = {Abstract, We have investigated the exciton Mott transition in a bulk Si by optical pump and terahertz probe spectroscopy under quasi-thermal equilibrium condition. The 1s-2p transition energy of excitons in Si that locates around 12 meV (?3 THz) is directly probed by the broadband terahertz pulses. The resonance energy of the 1s-2p transition stays unchanged with increasing the photo-excited electron-hole (e-h) pair density. The resonance structure sustains even above the Mott density, although broadened, in contrast to the conventional picture of the exciton Mott transition where the exciton binding energy vanishes toward the Mott density. The results indicate the existence of excitonic correlation in the metallic electron-hole plasma phase (? 2011 WILEY-VCH Verlag GmbH & Co. KGaA, Weinheim)},
  doi       = {10.1002/pssc.201000836},
  file      = {:PAPERS/pssc.201000836.pdf:PDF},
  groups    = {Bulks, Silicon, Time-Resolved Phenomena, Absorption under Pump},
  keywords  = {exciton, Mott transition, terahertz, Coulomb correlation, insulator to metal transition},
  publisher = {WILEY-VCH Verlag},
  url       = {http://dx.doi.org/10.1002/pssc.201000836},
}

