@Article{Sangalli2015,
  author   = {D. Sangalli and A. Marini},
  title    = {Ultra-fast carriers relaxation in bulk silicon following photo-excitation with a short and polarized laser pulse},
  journal  = {EPL (Europhysics Letters)},
  year     = {2015},
  volume   = {110},
  number   = {4},
  pages    = {47004},
  abstract = {We use an atomistic approach to provide a novel and ground-breaking interpretation of the ultra-fast carriers relaxation in a realistic material: bulk silicon. By comparing the results of ab initio simulations with recent two-photon photo-emission measurements we show that the description of the carrier relaxation in terms of ##IMG## [http://ej.iop.org/images/0295-5075/110/4/47004/epl17103ieqn1.gif] {$L\rightarrow X$} inter-valley scattering is not correct. The ultra-fast dynamics measured experimentally is, instead, due to the scattering between degenerate L states that is activated by the non-symmetric population of the conduction bands induced by the laser field. This ultra-fast relaxation is, then, entirely due to the specific experimental setup and it can be interpreted by introducing a novel definition of the quasi-particle lifetimes in an out-of-equilibrium context.},
  file     = {:PAPERS/epl17103-offprints.pdf:PDF},
  groups   = {Silicon, Time-Resolved Phenomena, Time-resolved relaxation},
  url      = {http://stacks.iop.org/0295-5075/110/i=4/a=47004},
}

