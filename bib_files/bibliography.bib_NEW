  file           = {:PAPERS/JChemPhys_126_191106.pdf:PDF},
  groups         = {Time-Resolved Phenomena, ChargeTransfer},
  keywords-plus  = {CHAINS},
  owner          = {myrta},
  times-cited    = {5},
  timestamp      = {2009.10.10},
