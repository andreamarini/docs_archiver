@Article{Lein2000a,
  author    = {Lein, Manfred and Gross, E. K. U. and Perdew, John P.},
  title     = {Electron correlation energies from scaled exchange-correlation kernels: Importance of spatial versus temporal nonlocality},
  journal   = {Phys. Rev. B},
  year      = {2000},
  volume    = {61},
  number    = {20},
  pages     = {13431--},
  month     = may,
  abstract  = {Within density functional theory, a coordinate-scaling relation for the coupling-constant dependence of the exchange-correlation kernel fxc(r,r?;?) is utilized to express the correlation energy of a many-electron system in terms of fxc. As a test of several of the available approximations for the exchange-correlation kernel, or equivalently the local-field factor, we calculate the uniform-gas correlation energy. While the random phase approximation (fxc = 0) makes the correlation energy per electron too negative by about 0.5 eV, the adiabatic local-density approximation [fxc = fxc(q = 0,? = 0)] makes a comparable error in the opposite direction. The adiabatic nonlocal approximation [fxc = fxc(q,? = 0)] reduces this error to about 0.1 eV, and inclusion of the full frequency dependence [fxc = fxc(q,?)] in an approximate parametrization reduces it further to less than 0.02 eV. We also report the wave-vector analysis and the imaginary-frequency analysis of the correlation energy for each choice of kernel.},
  file      = {:PAPERS/PhysRevB.61.13431.pdf:PDF},
  groups    = {Time-Resolved Phenomena, elGas},
  owner     = {myrta},
  publisher = {American Physical Society},
  refid     = {10.1103/PhysRevB.61.13431},
  timestamp = {2009.10.06},
  url       = {http://link.aps.org/abstract/PRB/v61/p13431},
}

