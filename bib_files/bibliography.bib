@Other{Maitra2006a,
    groups     	 = {Time-Resolved Phenomena, TDDFT},
    review     	 = {*Memory dependence: initial state dependence + history of the density dependence *Memory dependence comes from reducing the degree of freedom of the problem from 3N -> 3. *For initial state = non deg GS, initial state dependence contained implicit contained in history dependence (by virtue of HK theo) *In practice it is always possible to reduce to such a situation and to have the initial state dependence implicit in the history dependence  *Memory dependence should be important for: +quantum control, + strong field Vertical excitation generally good within the adiabatic approximation (not entirely clear why), except double excitation, for which memory dependence (that translate in the frequency dependence of the kernel) is essential!},
    pages      	 = {61--74},
    url        	 = {http://dx.doi.org/10.1007/3-540-35426-3_4},
    owner      	 = {myrta},
    journal    	 = {Time-Dependent Density Functional Theory},
    author     	 = {Maitra, N.T.},
    timestamp  	 = {2009.11.02},
    file       	 = {:PAPERS/MaitraTDDFTBook.pdf:PDF},
    abstract   	 = {In ground-state DFT, the fact that the xc potential is a functional of the density is a direct consequence of the one-to-one mapping between ground-state densities and potentials. In TDDFT, the one-to-one mapping is between densities and potentials for a given initial-state. This means that the potentials, most generally, are functionals of the initial state of the system, as well as of the density; and, not just of the instantaneous density, but of its entire history. These dependences are explicitly displayed in (1.33) of Chap. 1. Of particular interest is the xc potential, as that is the quantity that must be approximated. This is the difference between the KS potential and the sum of the external and Hartree potentials. The Hartree potential has no memory, as the classical Coulomb interaction depends on the instantaneous density only, but since both the interacting and non-interacting mappings can depend on the initial state, the xc potential must be a functional of both the initial states and density.},
    title      	 = {Initial-State Dependence and Memory},
    year       	 = {2006},
}
@Article{Maitra2005,
    keywords-plus   	 = {TRANSFER EXCITED-STATES; EXCITATION-ENERGIES; INTEGER DISCONTINUITY; NONLOCAL EXCHANGE; ORGANIC-COMPOUNDS; LINEAR-RESPONSE; APPROXIMATION; EIGENVALUES; PERFORMANCE; COMPLEXES},
    journal         	 = {JOURNAL OF CHEMICAL PHYSICS},
    issn            	 = {0021-9606},
    year            	 = {2005},
    file            	 = {:PAPERS/JChemPhys_122_234104.pdf:PDF},
    owner           	 = {myrta},
    number          	 = {23},
    pages           	 = {234104},
    volume          	 = {122},
    abstract        	 = {Long-range charge-transfer excited states are notoriously badly underestimated in time-dependent density-functional theory (TDDFT). We discuss how exact TDDFT captures charge transfer between open-shell species: in particular, the role of the step in the ground-state potential, and the severe frequency dependence of the exchange-correlation kernel. An expression for the latter is derived, that becomes exact in the limit that the charge-transfer excitations are well separated from other excitations. The exchange-correlation kernel has the task of undoing the static correlation in the ground state introduced by the step, in order to accurately recover the physical charge-transfer states. (C) 2005 American Institute of Physics.},
    author          	 = {Maitra, NT},
    doi             	 = {10.1063/1.1924599},
    title           	 = {Undoing static correlation: Long-range charge transfer in time-dependent density-functional theory},
    groups          	 = {Time-Resolved Phenomena, BeyondAdiabatic, Theory, ChargeTransfer},
    month           	 = {JUN 15},
    article-number  	 = {234104},
    unique-id       	 = {ISI:000230091400004},
    timestamp       	 = {2009.10.09},
    times-cited     	 = {39},
}
