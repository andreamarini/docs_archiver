    keywords-plus  	 = {SPIN COULOMB DRAG; LINEAR-RESPONSE; QUANTUM-WELLS; ELECTRON-GAS; APPROXIMATION; SPECTRA; SYSTEMS; TRANSPORT; MOLECULES; CLUSTERS},
    volume         	 = {5},
