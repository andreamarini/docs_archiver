@Article{Calegari2014a,
  author    = {Calegari, F. and Ayuso, D. and Trabattoni, A. and Belshaw, L. and De Camillis, S. and Anumula, S. and Frassetto, F. and Poletto, L. and Palacios, A. and Decleva, P. and Greenwood, J. B. and Mart\'\in, F. and Nisoli, M.},
  title     = {Ultrafast electron dynamics in phenylalanine initiated by attosecond pulses},
  journal   = {Science},
  year      = {2014},
  volume    = {346},
  number    = {6207},
  pages     = {336--339},
  issn      = {0036-8075},
  abstract  = {In the past decade, attosecond technology has opened up the investigation of ultrafast electronic processes in atoms, simple molecules, and solids. Here, we report the application of isolated attosecond pulses to prompt ionization of the amino acid phenylalanine and the subsequent detection of ultrafast dynamics on a sub\textendash4.5-femtosecond temporal scale, which is shorter than the vibrational response of the molecule. The ability to initiate and observe such electronic dynamics in polyatomic molecules represents a crucial step forward in attosecond science, which is progressively moving toward the investigation of more and more complex systems.Over the past decade, laser technology has pushed back the fastest directly observable time scale from femtoseconds (quadrillionths of a second) to attoseconds (quintillionths of a second). For the most part, attosecond studies so far have probed very simple molecules such as H2 and O2. Calegari et al. now look at a more elaborate molecule\textemdashthe amino acid phenylalanine. They tracked changes in the electronic structure of the compound after absorption of an ultrafast pulse, before the onset of conventional vibrational motion.Science, this issue p. 336},
  doi       = {10.1126/science.1254061},
  eprint    = {http://science.sciencemag.org/content/346/6207/336.full.pdf},
  file      = {:/home/marini/Papers_and_Results/DATABASE/ultrafast-electron-dynamics-in-phenylalanine-initiated-by-attosecond-pulses.pdf:PDF},
  groups    = {phenylalanine, Atto Chemistry},
  publisher = {American Association for the Advancement of Science},
  timestamp = {2018.09.28},
  url       = {http://science.sciencemag.org/content/346/6207/336},
}
