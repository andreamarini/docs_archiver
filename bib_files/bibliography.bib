@Article{Lee2008,
  title 	 = {Dynamical screening and memory effect of non-equilibrium carrier-carrier scattering in GaAs},
  journal 	 = {physica status solidi (b)},
  volume 	 = {245},
  number 	 = {4},
  doi 	 = {10.1002/pssb.200743362},
  author 	 = {Lee, Han-Chieh},
  groups 	 = {Time-Resolved Phenomena, Time-resolved relaxation},
  file 	 = {:/home/marini/Papers_and_Results/DATABASE/707_ftp.pdf:PDF},
  url 	 = {http://dx.doi.org/10.1002/pssb.200743362},
  publisher 	 = {WILEY-VCH Verlag},
  abstract 	 = {Given the absence of a Coulomb singularity in the quantum regime, the dependence of dynamical screening in non-equilibrium carrier?carrier scattering on the dimension of GaAs was examined using the generalized Kadanoff?Baym equation. The screening in a quantum well is stronger than that in the bulk but this has not been previously understood. This dependence also applies to the Fr?hlich interaction because, in momentum space, the electric flux density of small-angle scattering is stronger in a lower dimension. Calculations also revealed the memory effect; based on these, the quantum kinetic oscillation was conceptualized. As time evolves, the scattering rate increases at the early stage and causes hole burning in the carrier's distribution. This effect enables the dependence of the average relaxation time on the carrier density to become independent of the sample's dimension, unlike in the Boltzmann regime. This work can eliminate the apparent contradiction between Shank's and Haug's experimental results. Finally, an experimental problem regarding the anomalously fast thermalization of non-equilibrium carriers immersed in cold electrons in a quantum well was investigated and a reasonable explanation is presented. (? 2008 WILEY-VCH Verlag GmbH & Co. KGaA, Weinheim)},
  year 	 = {2008},
  pages 	 = {707--713},
  issn 	 = {1521-3951},
}
@Article{Lee2015,
  url 	 = {http://dx.doi.org/10.1039/C4NR05785F},
  issue 	 = {7},
  title 	 = {Anomalous excitonic resonance Raman effects in few-layered MoS2},
  publisher 	 = {The Royal Society of Chemistry},
  journal 	 = {Nanoscale},
  abstract 	 = {The resonance effects on the Raman spectra from 5 to 900 cm-1 of few-layered MoS2 thin films up to 14-layers were investigated by using six excitation energies. For the main first-order Raman peaks, the intensity maximum occurs at [similar]2.8 eV for single layered and at [similar]2.5 eV for few-layered MoS2, which correspond to the band-gap energy. At the excitation energy of 1.96 eV, several anomalous behaviors are observed. Many second-order peaks are anomalously enhanced even though the main first-order peaks are not enhanced. In the low-frequency region (<100 cm-1), a broad peak centered at [similar]38 cm-1 and its second order peak at 76 cm-1 appear for the excitation energy of 1.96 eV. These anomalous resonance effects are interpreted as occuring due to the strong resonance with excitons or exciton-polaritons.},
  year 	 = {2015},
  volume 	 = {7},
  pages 	 = {3229-3236},
  doi 	 = {10.1039/C4NR05785F},
  author 	 = {Lee, Jae-Ung and Park, Jaesung and Son, Young-Woo and Cheong, Hyeonsik},
