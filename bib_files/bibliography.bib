}
@Article{Thiele2009,
  doi 	 = {10.1103/PhysRevA.80.012514},
  number 	 = {1},
  author 	 = {M. Thiele and S. Kummel},
  groups 	 = {Time-Resolved Phenomena, ModelSystems, Exact_vs_Adiabatic},
  title 	 = {Reconstructing the adiabatic exchange-correlation kernel of time-dependent density-functional theory},
  numpages 	 = {12},
  volume 	 = {80},
  journal 	 = {Physical Review A (Atomic, Molecular, and Optical Physics)},
  pages 	 = {012514},
  eid 	 = {012514},
  keywords 	 = {density functional theory; electron correlations; helium neutral atoms},
  publisher 	 = {APS},
  url 	 = {http://link.aps.org/abstract/PRA/v80/e012514},
  file 	 = {:/home/marini/Papers_and_Results/DATABASE/PhysRevA_80_012514.pdf:PDF},
  year 	 = {2009},
  abstract 	 = {The interacting and the Kohn-Sham static density-density response functions for different one-dimensional two-electron singlet systems are reconstructed numerically. From their inverse we obtain the exact static exchange-correlation kernel. This quantity represents the adiabatically exact approximation of the frequency-dependent exchange-correlation kernel that is crucial for time-dependent linear density-response theory. We investigate its performance for nonlocal perturbations and analyze its sum rule properties. We also compute the adiabatically exact transition energies that follow from the static kernel within linear-response theory.},
}
@Article{Thiele2009a,
  year 	 = {2009},
  abstract 	 = {The adiabatic approximation of time-dependent density-functional theory is studied in the context of nonlinear excitations of two-electron singlet systems. We compare the exact time evolution of these systems to the adiabatically exact one obtained from time-dependent Kohn-Sham calculations relying on the exact ground-state exchange-correlation potential. Thus, we can show under which conditions the adiabatic approximation breaks down and memory effects become important. The hydrodynamic formulation of quantum mechanics allows us to interpret these results and relate them to dissipative effects in the Kohn-Sham system. We show how the breakdown of the adiabatic approximation can be inferred from the rate of change of the ground-state noninteracting kinetic energy.},
  publisher 	 = {APS},
  url 	 = {http://link.aps.org/abstract/PRA/v79/e052503},
  file 	 = {:/home/marini/Papers_and_Results/DATABASE/PhysRevA_79_052503.pdf:PDF},
  keywords 	 = {density functional theory; excited states; ground states; hydrodynamics; quantum theory},
  eid 	 = {052503},
  pages 	 = {052503},
  volume 	 = {79},
  numpages 	 = {12},
  journal 	 = {Physical Review A (Atomic, Molecular, and Optical Physics)},
  title 	 = {Hydrodynamic perspective on memory in time-dependent density-functional theory},
  timestamp 	 = {2018.09.28},
  comment 	 = {Condition and quantitative criterion for breakdown of adiabatic approximation. Analysis based either on exact-adiabatic comparison for 1D 2-el system, and hydrodynamic picture. As in the PRL here the Vext,o is built from inversion of SE to get the exact adiabatic approximation. Memory effects appears when strong rarefaction/compression of the density appears (dissipative effects in the KS system). Criterion based on GS non-interacting kin energy.},
