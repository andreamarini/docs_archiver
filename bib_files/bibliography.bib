@Article{Gritsenko2009,
    journal        	 = {PHYSICAL CHEMISTRY CHEMICAL PHYSICS},
    groups         	 = {Double Excitations, Time-Resolved Phenomena, BeyondAdiabatic},
    review         	 = {Extension/Generalization of the frequency dependence needed in the kernel to describe double excitation described in Maitra et al (JCP 2004). It includes the coupling of the selected double character excitations to the others excitations by using a common energy denominator approximation},
    doi            	 = {10.1039/b903123e},
    year           	 = {2009},
    file           	 = {:PAPERS/Gritsenko2009.pdf:PDF},
    unique-id      	 = {ISI:000266587300026},
    keywords-plus  	 = {DISSOCIATING H-2; SYSTEMS},
    owner          	 = {myrta},
    number         	 = {22},
    times-cited    	 = {0},
    volume         	 = {11},
    timestamp      	 = {2009.10.09},
    issn           	 = {1463-9076},
    title          	 = {Double excitation effect in non-adiabatic time-dependent density functional theory with an analytic construction of the exchange-correlation kernel in the common energy denominator approximation},
    pages          	 = {4640-4646},
    author         	 = {Gritsenko, Oleg V. and Baerends, Evert Jan},
    abstract       	 = {Time-dependent density functional (response) theory (TDDF(R)T) is applied almost exclusively in its adiabatic approximation (ATDDFT), which is restricted to predominantly single electronic excitations and neglects additional roots of the TDDFT eigenvalue problem stemming from the interaction between single and double excitations. We incorporate the effect of the latter interaction into a non-adiabatic frequency-dependent and spatially non-local Hartree-exchange-correlation (Hxc) kernel f(Hxc)(CEDA) (r(1), r(2), omega), the explicit analytical expression of which is derived for interacting single and double excitations well separated from the other excitations, within the common energy denominator approximation (CEDA) for the Kohn-Sham (KS) and interacting density response functions, chi(s) and chi, respectively. The kernel f(Hxc)(CEDA) (r(1), r(2), omega) obtained from the direct analytical inverse of chi(CEDA)(s) and chi(CEDA) is a sum of the delta-function and non-local orbital-dependent spatial terms with frequency-dependent factors, with which f(Hxc)(CEDA) acquires a modulated quadratic dependence on omega. The effective incorporation in f(Hxc)(CEDA) of the complete manifold of excited states (through the delta function term) represents an extension of the kernel reported by Maitra, Zhang, Cave, and Burke [J. Chem. Phys., 2004, 120, 5932]. In the TDDFT eigenvalue equations considered in the diagonal approximation, f(Hxc)(CEDA) generates two excitation energies omega(q) and omega(q+1), which both correspond to the same single KS excitation omega(s)(q), thus producing the effect of the single-double excitation interaction.},
}
