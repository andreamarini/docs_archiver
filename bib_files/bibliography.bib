  file 	 = {:/home/marini/Papers_and_Results/DATABASE/bechstedt_memory_effects.pdf:PDF},
  title 	 = {Nonequilibrium photoluminescence excitation spectroscopy in GaAs: Bottleneck and memory effects},
}
@Article{K.2002,
  title 	 = {Femtosecond Dynamics of Luminescence in Optically Excited Semiconductors: Theory and Simulation},
  timestamp 	 = {2009.12.13},
  file 	 = {:/home/marini/Papers_and_Results/DATABASE/bechstedt_theory_and_simulation.pdf:PDF},
  volume 	 = {190},
  year 	 = {2002},
  abstract 	 = {We present a theory of time- and energy-resolved photoluminescence (PL) from semiconductors excited by femtosecond laser pulses. Our approach combines quantum kinetics of hot-carrier relaxation and quantum theory of spontaneous emission under consistent inclusion of Coulomb interaction. Model calculations show the transition from PL at the pump frequency via subsequent phonon replicas until the build-up of excitonic PL. We predict hot luminescence to be a sensitive measure of electron?LO-phonon quantum kinetics and bottleneck effects.},
  journal 	 = {phys. stat. sol. (a)},
  pages 	 = {839?842},
  owner 	 = {marini},
  groups 	 = {Photo Luminescence, Time-Resolved Phenomena},
  author 	 = {K. Hannewald,S. Glutsch,F. Bechstedt},
}
