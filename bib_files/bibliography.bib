@Article{Thiele2009a,
    numpages   	 = {12},
    abstract   	 = {The adiabatic approximation of time-dependent density-functional theory is studied in the context of nonlinear excitations of two-electron singlet systems. We compare the exact time evolution of these systems to the adiabatically exact one obtained from time-dependent Kohn-Sham calculations relying on the exact ground-state exchange-correlation potential. Thus, we can show under which conditions the adiabatic approximation breaks down and memory effects become important. The hydrodynamic formulation of quantum mechanics allows us to interpret these results and relate them to dissipative effects in the Kohn-Sham system. We show how the breakdown of the adiabatic approximation can be inferred from the rate of change of the ground-state noninteracting kinetic energy.},
    file       	 = {:PAPERS/PhysRevA_79_052503.pdf:PDF},
    review     	 = {Condition and quantitative criterion for breakdown of adiabatic approximation. Analysis based either on exact-adiabatic comparison for 1D 2-el system, and hydrodynamic picture. As in the PRL here the Vext,o is built from inversion of SE to get the exact adiabatic approximation. Memory effects appears when strong rarefaction/compression of the density appears (dissipative effects in the KS system). Criterion based on GS non-interacting kin energy.},
    keywords   	 = {density functional theory; excited states; ground states; hydrodynamics; quantum theory},
    author     	 = {M. Thiele and S. Kummel},
