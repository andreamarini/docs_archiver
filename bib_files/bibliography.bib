  groups 	 = {Dichalcogenides, hBN, Time-Resolved Phenomena},
  author 	 = {Kim, Jonghwan and Hong, Xiaoping and Jin, Chenhao and Shi, Su-Fei and Chang, Chih-Yuan S. and Chiu, Ming-Hui and Li, Lain-Jong and Wang, Feng},
  number 	 = {6214},
  doi 	 = {10.1126/science.1258122},
  journal 	 = {Science},
  volume 	 = {346},
  eprint 	 = {http://science.sciencemag.org/content/346/6214/1205.full.pdf},
  title 	 = {Ultrafast generation of pseudo-magnetic field for valley excitons in WSe2 monolayers},
}
