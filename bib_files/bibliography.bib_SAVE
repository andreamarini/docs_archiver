@Article{Hesselmann2009,
  Title                    = {Charge-transfer excitation energies with a time-dependent density-functional method suitable for orbital-dependent exchange-correlation kernels},
  Author                   = {Hesselmann, Andreas and Ipatov, Andrey and Goerling, Andreas},
  Journal                  = {PHYSICAL REVIEW A},
  Year                     = {2009},

  Month                    = {JUL},
  Number                   = {1},
  Pages                    = {012507},
  Volume                   = {80},

  Abstract                 = {A time-dependent density-functional (TDDFT) response equation for the effective Kohn-Sham potential instead of the electron densities is presented that enables the use of orbital-dependent exchange-correlation kernels. In combination with the frequency-dependent exact-exchange kernel the present approach describes long-range charge-transfer excitations qualitatively correct in contrast to standard TDDFT methods, as shown by formal analysis and applications to molecular systems. Even charge-transfer excitations obtained with the adiabatic frequency-independent exact-exchange kernel exhibit a long-range dependence with the distance of the charge transfer, which, however, is too weak by a factor of 2. This indicates that it is not the frequency dependence of the kernel alone that leads to a correct description of charge-transfer excitations.},
  Article-number           = {012507},
  Doi                      = {10.1103/PhysRevA.80.012507},
  File                     = {:PAPERS/PhysRevA_80_012507.pdf:PDF},
  ISSN                     = {1050-2947},
  Keywords-plus            = {SPATIALLY SEPARATED SYSTEMS; STRONG MAGNETIC-FIELDS; NONLOCAL EXCHANGE; BASIS-SETS; FAILURE; APPROXIMATION; MOLECULES; STATES; ERROR},
  Owner                    = {myrta},
  Times-cited              = {0},
  Timestamp                = {2009.10.09},
  Unique-id                = {ISI:000268616900085}
}

@Article{Hessler2002,
  Title                    = {Correlation in time-dependent density-functional theory},
  Author                   = {Paul Hessler and Neepa T. Maitra and Kieron Burke},
  Journal                  = {The Journal of Chemical Physics},
  Year                     = {2002},
  Number                   = {1},
  Pages                    = {72-81},
  Volume                   = {117},

  Abstract                 = {The exact time-dependent correlation energy in time-dependent density-functional theory is shown to sometimes become positive, which is impossible with most present TDDFT approximations. Both the correlation potential and energy can be strongly nonlocal in time. A new inequality is derived for the time-dependent exchange-correlation energy. The correlation energy appears to scale to a constant function of scaled time in the high-density limit. In the linear response regime, the correlation energy is shown to become purely adiabatic, but the correlation potential is generally nonadiabatic. The usefulness of the virial theorem as a test of numerical accuracy is demonstrated. All results are found or inspired by exact numerical solution of a simple model system (Hooke's atom), and inversion of the corresponding Kohn?Sham equations.},
  Doi                      = {10.1063/1.1479349},
  File                     = {:PAPERS/JChemPhys_117_72.pdf:PDF},
  Keywords                 = {density functional theory},
  Publisher                = {AIP},
  Url                      = {http://link.aip.org/link/?JCP/117/72/1}
}

