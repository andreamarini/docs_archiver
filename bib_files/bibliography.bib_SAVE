@Article{Henneberger1986a,
  Title                    = {Nonequilibrium green's functions and kinetic equations for highly excited semiconductors: II. Application to the study of nonlinear optical and transport properties of the many-exciton system },
  Author                   = {K. Henneberger and G. Manzke and V. May and R. Zimmermann},
  Journal                  = {Physica A: Statistical Mechanics and its Applications },
  Year                     = {1986},
  Number                   = {3},
  Pages                    = {557 - 572},
  Volume                   = {138},

  Abstract                 = {Nonequilibrium Green's function technique is applied to the many-exciton system under the action of an externally driven light field. Starting with Dyson's equation for the nonequilibrium exciton propagator the shift and damping of exciton-levels due to exciton-exciton interaction are calculated in a local approximation with respect to the density of excitons. As a consequence, the Boltzmann equation of excitons contains many-exciton contributions in the diffusion- and drift-term as well as in the collision integral. The corresponding diffusion equation is derived yielding a diffusion coefficient decreasing with increasing density and a density dependent source term due to the action of the light field. Numerical calculations are carried out for An = 1-excitons of a CdS platelet considering a two-beam as well as a one-beam experiment. For the two-beam case we present the density-profile resulting from nonlinear diffusion and the corresponding reflection spectrum around the An = 1-level. For the one-beam case solving the diffusion equation and Maxwell's equations simultaneously an optical bistability just below the Mott-transition is predicted. },
  Doi                      = {http://dx.doi.org/10.1016/0378-4371(86)90033-6},
  File                     = {:PAPERS/1-s2.0-0378437186900336-main.pdf:PDF},
  ISSN                     = {0378-4371},
  Url                      = {http://www.sciencedirect.com/science/article/pii/0378437186900336}
}

@Article{Henneberger1986,
  Title                    = {Nonequilibrium green's functions and kinetic equations for highly excited semiconductors: I. General considerations },
  Author                   = {K. Henneberger and V. May},
  Journal                  = {Physica A: Statistical Mechanics and its Applications },
  Year                     = {1986},
  Number                   = {3},
  Pages                    = {537 - 556},
  Volume                   = {138},

  Abstract                 = {Aiming at a realistic description of highly excited states in semiconductors the derivation of kinetic equations is reformulated where emphasis is laid on the consideration of many-body effects without perturbation expansion arguments. By the variational derivation technique a set of equations for the Green's functions and the self-energy is obtained, which is formally closed and should be an appropriate starting point for any kind of iteration or approximation. The connection of this technique with the diagram technique given by Keldysh and the translation technique for thermodynamic Green's functions according to Kadanoff and Baym is demonstrated. The general equations are then exactly transformed to difference and sum coordinates, enabling an adequate approximation in the case of slowly varuing (in space and time) external fields in terms of local quantities. In linear approximation with respect to the drift operator DÃÂ a generalized Boltzmann equation is derived, which clearly exhibits many-body effects in all drift and collision contributions.},
  Doi                      = {http://dx.doi.org/10.1016/0378-4371(86)90032-4},
  File                     = {:PAPERS/1-s2.0-0003491684900927-main.pdf:PDF},
  ISSN                     = {0378-4371},
  Url                      = {http://www.sciencedirect.com/science/article/pii/0378437186900324}
}

