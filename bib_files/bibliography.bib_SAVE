@Article{Patrick2014,
  Title                    = {Unified theory of electronÃ¢ÂÂphonon renormalization and phonon-assisted optical absorption},
  Author                   = {Christopher E Patrick and Feliciano Giustino},
  Journal                  = {Journal of Physics: Condensed Matter},
  Year                     = {2014},
  Number                   = {36},
  Pages                    = {365503},
  Volume                   = {26},

  Abstract                 = {We present a theory of electronic excitation energies and optical absorption spectra which incorporates energy-level renormalization and phonon-assisted optical absorption within a unified framework. Using time-independent perturbation theory we show how the standard approaches for studying vibronic effects in molecules and those for addressing electronÃ¢ÂÂphonon interactions in solids correspond to slightly different choices for the non-interacting Hamiltonian. Our present approach naturally leads to the AllenÃ¢ÂÂHeine theory of temperature-dependent energy levels, the FranckÃ¢ÂÂCondon principle, the HerzbergÃ¢ÂÂTeller effect and to phonon-assisted optical absorption in indirect band gap materials. In addition, our theory predicts sub-gap phonon-assisted optical absorption in direct gap materials, as well as an exponential edge which we tentatively assign to the Urbach tail. We also consider a semiclassical approach to the calculation of optical absorption spectra which simultaneously captures energy-level renormalization and phonon-assisted transitions and is especially suited to first-principles electronic structure calculations. We demonstrate this approach by calculating the phonon-assisted optical absorption spectrum of bulk silicon.},
  File                     = {:PAPERS/0953-8984_26_36_365503.pdf:PDF},
  Url                      = {http://stacks.iop.org/0953-8984/26/i=36/a=365503}
}

