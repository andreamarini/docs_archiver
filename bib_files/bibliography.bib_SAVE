@Article{Thiele2008,
  Title                    = {Adiabatic Approximation in Nonperturbative Time-Dependent Density-Functional Theory},
  Author                   = {M. Thiele and E. K. U. Gross and S. Kummel},
  Journal                  = {Physical Review Letters},
  Year                     = {2008},
  Number                   = {15},
  Pages                    = {153004},
  Volume                   = {100},

  Abstract                 = {We construct the exact exchange-correlation potential of time-dependent density-functional theory and the approximation to it that is adiabatic but exact otherwise. For the strong-field double ionization of the Helium atom these two potentials are virtually identical. Thus, memory effects play a negligible role in this paradigm process of nonlinear, nonperturbative electron dynamics. We identify the regime of high-frequency excitations where the adiabatic approximation breaks down and explicitly calculate the nonadiabatic contribution to the exchange-correlation potential.},
  Doi                      = {10.1103/PhysRevLett.100.153004},
  Eid                      = {153004},
  File                     = {:PAPERS/PhysRevLett_100_153004.pdf:PDF},
  Numpages                 = {4},
  Publisher                = {APS},
  Review                   = {The main result of the work is that, contrary to what commonly thought, memory effects are negligible for strong-field excitations.
This conclusion is based on numerical experiments on 1D He in which the exact TD is compared with the exact adiabatic potential Vxc.
* the exact Vs (and thus Vxc) is found by inversion of the TDKS equation.
* the exact adiabatic Vxc instead requires the GS Vs corresponding to the TD density at a given t, considered as GS density, and the GS external potential corresponding to the same density (that will be different from the "real" external TD potential)
The three systems are:
A. He in dc electric field
B. He with a forcing that mimics nuclei motion
C. He in strong, time-dependent external ?elds due to powerful laser pulses (780 nm)
In the first two cases the TDSE is solved and at every step from the corresponding TD density the exact and adiabatic is found
In the third cases to assess the accuracy of the adiabatically exact approxi-
mation in practice: propagate the KS orbital while using at every time step the adiabatically exact approximation of vc that is obtained self-consistently from the KS density.
In A. Vc adiabatic = Vc exact
In B Vc adiabatic \= Vc exact 
In C same quantitative ionization behavior
Questions:
How is the adiabatic potential found in C? (not really clear) Is the GS external potential corresponding to GS density still obtained by inversion of the SE?? In this case to construct the trajectory in the adiabatic case we need not only Vc, but also Vext (not known and not possible in general to obtain). If this is the case, we could expect this results from A},
  Url                      = {http://link.aps.org/abstract/PRL/v100/e153004}
}

@Article{Thiele2009b,
  Title                    = {Photoabsorption spectra from adiabatically exact time-dependent density-functional theory in real time},
  Author                   = {Mark Thiele and Stephan K?mmel},
  Journal                  = {Phys. Chem. Chem. Phys.},
  Year                     = {2009},
  Pages                    = {4631-4639},
  Volume                   = {11},

  Abstract                 = {Photoabsorption spectra for 2-electron singlet systems are obtained from the real-time propagation of the time-dependent Kohn?Sham equations in the adiabatically exact approximation. The latter is provided by the exact ground state exchange?correlation potential corresponding to the instantaneous density. The results are compared to exact data obtained from the solution of the interacting Schr?dinger equation. We find that the adiabatically exact approximation provides very good results for transitions of genuinely single excitation character but yields incorrect results if double excitations contribute substantially. However, the extent of the error can vary: some double excitations are just shifted in energy whereas others are missed completely. These situations are analyzed with the help of transition densities.},
  Doi                      = {10.1039/b902567g},
  File                     = {:PAPERS/PCCP_11_4631_2009.pdf:PDF},
  Owner                    = {myrta},
  Timestamp                = {2009.10.06},
  Url                      = {http://www.rsc.org/publishing/journals/CP/article.asp?doi=b902567g}
}

@Article{Thiele2009,
  Title                    = {Reconstructing the adiabatic exchange-correlation kernel of time-dependent density-functional theory},
  Author                   = {M. Thiele and S. Kummel},
  Journal                  = {Physical Review A (Atomic, Molecular, and Optical Physics)},
  Year                     = {2009},
  Number                   = {1},
  Pages                    = {012514},
  Volume                   = {80},

  Abstract                 = {The interacting and the Kohn-Sham static density-density response functions for different one-dimensional two-electron singlet systems are reconstructed numerically. From their inverse we obtain the exact static exchange-correlation kernel. This quantity represents the adiabatically exact approximation of the frequency-dependent exchange-correlation kernel that is crucial for time-dependent linear density-response theory. We investigate its performance for nonlocal perturbations and analyze its sum rule properties. We also compute the adiabatically exact transition energies that follow from the static kernel within linear-response theory.},
