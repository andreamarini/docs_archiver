@Article{Marini2013,
  Title                    = {Competition between the electronic and phononÃ¢ÂÂmediated scattering channels in the outÃ¢ÂÂofÃ¢ÂÂequilibrium carrier dynamics of semiconductors: an ab-initio approach},
  Author                   = {Andrea Marini},
