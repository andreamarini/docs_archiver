@Article{Giesbertz2008a,
  Title                    = {Failure of time-dependent density functional theory for excited state surfaces in case of homolytic bond dissociation},
  Author                   = {Giesbertz, K. J. H. and Baerends, E. J.},
  Journal                  = {Chemical Physics Letters},
